    "version": "0.2.0",
    "configurations": [
        {
            "name": "Serve DNS",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceRoot}/main.go",
            "env": {
                "http_proxy": "10.10.10.254:3128",
                "https_proxy": "10.10.10.254:3128"
            },
            "args": [
                "dns",
                "--insecure",
                "--port",
                "1053"
            ]
        }
    ]
}